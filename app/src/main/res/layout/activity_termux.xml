<com.termux.app.terminal.TermuxActivityRootView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_termux_root_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:fitsSystemWindows="true"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/activity_termux_root_relative_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="3dp"
            android:layout_marginVertical="0dp"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:id="@+id/rl_1"
                android:layout_above="@+id/terminal_toolbar_view_pager"
                android:layout_height="match_parent">
                <FrameLayout
                    android:layout_width="match_parent"
                    android:id="@+id/xue_fragment"
                    android:layout_height="match_parent"/>
                <androidx.drawerlayout.widget.DrawerLayout
                    android:id="@+id/drawer_layout"
                    android:layout_width="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_height="match_parent">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <com.termux.view.TerminalView
                            android:id="@+id/terminal_view"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginRight="3dp"
                            android:layout_marginLeft="3dp"
                            android:layout_above="@+id/key_bord"
                            android:focusableInTouchMode="true"
                            android:scrollbarThumbVertical="@drawable/terminal_scroll_shape"
                            android:scrollbars="vertical"
                            android:importantForAutofill="no"
                            android:autofillHints="password" />
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:id="@+id/key_bord"
                            android:layout_alignParentBottom="true"
                            android:orientation="vertical"
                            android:layout_height="wrap_content"/>
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/left_drawer"
                        android:layout_width="240dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="end"
                        android:background="@drawable/shape_r_3dp_161823"
                        android:choiceMode="singleChoice"
                        android:divider="@android:color/transparent"
                        android:dividerHeight="0dp"
                        android:descendantFocusability="blocksDescendants"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <ImageButton
                                android:id="@+id/settings_button"
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:src="@drawable/ic_settings"
                                android:background="@null"
                                android:contentDescription="@string/action_open_settings" />
                        </LinearLayout>
                        <ListView
                            android:id="@+id/terminal_sessions_list"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_gravity="top"
                            android:layout_weight="1"
                            android:choiceMode="singleChoice"
                            android:longClickable="true" />
                        <LinearLayout
                            style="?android:attr/buttonBarStyle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <Button
                                android:id="@+id/toggle_keyboard_button"
                                style="?android:attr/buttonBarButtonStyle"
                                android:layout_width="match_parent"
                                android:textColor="@color/color_ffffff"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/action_toggle_soft_keyboard" />
                            <Button
                                android:id="@+id/new_session_button"
                                style="?android:attr/buttonBarButtonStyle"
                                android:textColor="@color/color_ffffff"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/action_new_session" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/left_drawer_menu"
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="start"
                        android:choiceMode="singleChoice"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/shape_r_3dp_161823_left"
                        android:divider="@android:color/transparent"
                        android:dividerHeight="0dp"
                        android:descendantFocusability="blocksDescendants"
                        android:orientation="vertical">
                        <include layout="@layout/layout_menu_all"/>
                    </LinearLayout>
                </androidx.drawerlayout.widget.DrawerLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:text="@string/请双击命令窗口试试"
                    android:textColor="@color/color_ffffff"
                    android:gravity="center"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:id="@+id/double_tishi"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:alpha="0.1"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_alignParentRight="true"
                    android:id="@+id/title_mb"
                    android:orientation="vertical"
                    android:layout_height="wrap_content">
                    <androidx.cardview.widget.CardView
                        android:layout_width="200dp"
                        android:layout_marginLeft="5dp"
                        android:paddingTop="5dp"
                        android:layout_marginTop="10dp"
                        android:paddingBottom="5dp"
                        android:layout_marginRight="10dp"
                        android:visibility="visible"
                        app:cardElevation="0dp"
                        app:cardBackgroundColor="@color/color_161823"
                        app:cardCornerRadius="3dp"
                        android:layout_height="90dp">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingLeft="6dp"
                            android:paddingRight="6dp"
                            android:id="@+id/zt_title"
                            android:paddingTop="6dp"
                            android:paddingBottom="6dp"
                            android:layout_height="match_parent">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:textColor="@color/color_ffffff"
                                    android:textStyle="bold"
                                    android:textSize="25sp"
                                    android:text="Z"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:textColor="@color/color_ffffff"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:text="ero"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:textColor="@color/color_ffffff"
                                    android:textStyle="bold"
                                    android:textSize="25sp"
                                    android:text="T"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:textColor="@color/color_ffffff"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:text="ermux"
                                    android:layout_height="wrap_content"/>
                            </LinearLayout>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:textColor="@color/color_ffffff"
                                    android:textStyle="bold"
                                    android:textSize="15sp"
                                    android:id="@+id/version"
                                    android:layout_alignParentRight="true"
                                    android:text="@string/版本"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_below="@+id/version"
                                    android:textColor="@color/color_ffffff"
                                    android:textStyle="bold"
                                    android:id="@+id/eg_tv"
                                    android:textSize="15sp"
                                    android:layout_marginTop="5dp"
                                    android:layout_alignParentRight="true"
                                    android:text="@string/版本"
                                    android:layout_height="wrap_content"/>
                            </RelativeLayout>
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:layout_width="200dp"
                        android:layout_marginLeft="5dp"
                        android:paddingTop="5dp"
                        android:layout_marginTop="10dp"
                        android:paddingBottom="5dp"
                        android:layout_marginRight="10dp"
                        android:visibility="visible"
                        app:cardElevation="0dp"
                        app:cardBackgroundColor="@color/color_161823"
                        app:cardCornerRadius="3dp"
                        android:layout_height="90dp">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingLeft="6dp"
                            android:paddingRight="6dp"
                            android:paddingTop="6dp"
                            android:paddingBottom="6dp"
                            android:layout_height="match_parent">
                            <TextView
                                android:layout_width="wrap_content"
                                android:textColor="@color/color_ffffff"
                                android:textStyle="bold"
                                android:textSize="18sp"
                                android:layout_marginTop="5dp"
                                android:layout_alignParentRight="true"
                                android:text="@string/局域服务器"
                                android:layout_height="wrap_content"/>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:textColor="@color/color_ffffff"
                                    android:textStyle="bold"
                                    android:textSize="15sp"
                                    android:id="@+id/service_status"
                                    android:layout_alignParentRight="true"
                                    android:text="@string/正在连接"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:textColor="@color/color_ffffff"
                                    android:textStyle="bold"
                                    android:textSize="15sp"
                                    android:layout_below="@+id/service_status"
                                    android:id="@+id/service_eg"
                                    android:layout_marginTop="5dp"
                                    android:layout_alignParentRight="true"
                                    android:text="@string/正在连接"
                                    android:layout_height="wrap_content"/>
                            </RelativeLayout>
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:layout_width="200dp"
                        android:layout_marginLeft="5dp"
                        android:paddingTop="5dp"
                        android:layout_marginTop="10dp"
                        android:paddingBottom="5dp"
                        android:layout_marginRight="10dp"
                        android:visibility="visible"
                        app:cardElevation="0dp"
                        app:cardBackgroundColor="@color/color_161823"
                        app:cardCornerRadius="3dp"
                        android:layout_height="80dp">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingLeft="6dp"
                            android:paddingRight="6dp"
                            android:paddingTop="6dp"
                            android:paddingBottom="6dp"
                            android:layout_height="match_parent">
                            <TextView
                                android:layout_width="wrap_content"
                                android:textColor="@color/color_ffffff"
                                android:textStyle="bold"
                                android:textSize="18sp"
                                android:layout_marginTop="5dp"
                                android:layout_alignParentRight="true"
                                android:text="@string/本机IP"
                                android:layout_height="wrap_content"/>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:textColor="@color/color_ffffff"
                                    android:textStyle="bold"
                                    android:textSize="15sp"
                                    android:id="@+id/ip_status"
                                    android:layout_marginTop="15dp"
                                    android:layout_alignParentRight="true"
                                    android:text="@string/正在连接"
                                    android:layout_height="wrap_content"/>
                            </RelativeLayout>
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:layout_width="200dp"
                        android:layout_marginLeft="5dp"
                        android:paddingTop="5dp"
                        android:layout_marginTop="10dp"
                        android:paddingBottom="5dp"
                        android:layout_marginRight="10dp"
                        android:visibility="visible"
                        app:cardElevation="0dp"
                        app:cardBackgroundColor="@color/color_161823"
                        app:cardCornerRadius="3dp"
                        android:layout_height="200dp">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <ScrollView
                                android:layout_width="match_parent"
                                android:layout_height="200dp">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:orientation="vertical"
                                    android:paddingLeft="6dp"
                                    android:paddingRight="6dp"
                                    android:paddingTop="6dp"
                                    android:paddingBottom="6dp"
                                    android:layout_height="match_parent">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:orientation="horizontal"
                                        android:layout_height="wrap_content">
                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:text="@string/qq_group"
                                            android:gravity="center"
                                            android:textSize="12sp"
                                            android:id="@+id/qq_group_tv"
                                            android:textStyle="bold"
                                            android:textColor="@color/color_48baf3"
                                            android:layout_height="wrap_content"/>
                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:text="@string/telegram_group"
                                            android:gravity="center"
                                            android:textSize="12sp"
                                            android:id="@+id/telegram_group_tv"
                                            android:textStyle="bold"
                                            android:textColor="@color/color_48baf3"
                                            android:layout_height="wrap_content"/>
                                    </LinearLayout>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:textColor="@color/color_ffffff"
                                        android:textStyle="bold"
                                        android:textSize="12sp"
                                        android:id="@+id/msg_tv"
                                        android:layout_marginTop="5dp"
                                        android:layout_alignParentRight="true"
                                        android:text="@string/会话窗口在右侧"
                                        android:layout_height="wrap_content"/>
                                </LinearLayout>
                            </ScrollView>
                            <ImageView
                                android:layout_width="10dp"
                                android:src="@mipmap/up_down"
                                android:rotation="180"
                                android:layout_margin="2dp"
                                android:layout_alignParentRight="true"
                                android:layout_height="10dp"/>
                            <ImageView
                                android:layout_width="10dp"
                                android:src="@mipmap/up_down"
                                android:layout_margin="2dp"
                                android:layout_alignParentBottom="true"
                                android:layout_alignParentRight="true"
                                android:layout_height="10dp"/>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>
                </LinearLayout>
            </RelativeLayout>
            <androidx.viewpager.widget.ViewPager
                android:id="@+id/terminal_toolbar_view_pager"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="37.5dp"
                android:background="@android:drawable/screen_background_dark_transparent"
                android:layout_alignParentBottom="true" />
        </RelativeLayout>
        <View
            android:id="@+id/activity_termux_bottom_space_view"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/transparent" />
    </LinearLayout>

</com.termux.app.terminal.TermuxActivityRootView>
